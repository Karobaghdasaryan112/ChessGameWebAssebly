{
  "ReverseProxy": {
    "Routes": {
      "route1": {
        "ClusterId": "cluster1",
        "Match": {
          "Path": "api/Chat/{**catch-all}"
        }
      },
      "route2": {
        "ClusterId": "cluster2",
        "Match": {
          "Path": "api/chessgame/{**catch-all}"
        }
      },
      "route3": {
        "ClusterId": "cluster3",
        "Match": {
          "Path": "gamehub/{**catch-all}"
        }
      },
      "route4": {
        "ClusterId": "cluster4",
        "Match": {
          "Path": "/api/Identity/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "cluster1": {
        "Destinations": {
          "destination1": {
            "Address": "http://localhost:5001/"
          }
        }
      },
      "cluster2": {
        "Destinations": {
          "destination2": {
            "Address": "http://localhost:5002/"
          }
        }
      },
      "cluster3": {
        "Destinations": {
          "destination3": {
            "Address": "http://localhost:5203/gamehub"
          }
        }
      },
      "cluster4": {
        "Destinations": {
          "destination4": {
            "Address": "http://localhost:7287/"
          }
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
